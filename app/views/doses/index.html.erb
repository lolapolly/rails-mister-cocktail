<h1>Doses</h1>
<% @doses.each do |dose| %>
<ul>
  <li><%= dose.ingredient.name %></li>
  <li><%= dose.description %></li>
  <%= link_to dose_path(dose), method: :delete, data: { confirm: 'Are you sure?' } do %>
  <i class="fas fa-trash"></i>
  <% end %>
</ul>
<% end %>

<%= link_to 'Back', cocktail_path(@cocktail)%>
