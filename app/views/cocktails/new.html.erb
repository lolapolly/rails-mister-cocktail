<div class="white-box">
  <h2 class="">Create a new cocktail</h2>
    <%= simple_form_for @cocktail do |f|%>
      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit "add a cocktail", class: "btn btn-purple-border" %>
    <% end %>
</div>

